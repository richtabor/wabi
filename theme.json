{
	"version": 2,
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank",
			"postTypes": [
				"page",
				"post"
			]
		},
		{
			"name": "template-no-title",
			"title": "Page, No Title",
			"postTypes": [
				"page"
			]
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
                        "defaultPalette": false,
                        "defaultGradients": false,
			"duotone": [
				{
					"colors": [
						"#000000",
						"#abacca"
					],
					"slug": "black-and-primary",
					"name": "Black and primary"
				},
				{
					"colors": [
						"#000000",
						"#a8bed2"
					],
					"slug": "black-and-secondary",
					"name": "Black and secondary"
				},
				{
					"colors": [
						"#000000",
						"#a4beb2"
					],
					"slug": "black-and-tertiary",
					"name": "Black and Tertiary"
				},
				{
					"colors": [
						"#000000",
						"#eaca96"
					],
					"slug": "black-and-quarternary",
					"name": "Black and Quarternary"
				},
				{
					"colors": [
						"#000000",
						"#e9aa95"
					],
					"slug": "black-and-quinary",
					"name": "Black and Quinary"
				},
				{
					"colors": [
						"#000000",
						"#eeb8b0"
					],
					"slug": "black-and-senary",
					"name": "Black and Senary"
				},
				{
					"colors": [
						"#000000",
						"#ffffff"
					],
					"slug": "black-and-white",
					"name": "Black and white"
				},
				{
					"colors": [
						"#ffffff",
						"#e7e7e9"
					],
					"slug": "background-and-background-alt",
					"name": "Background and Background Alt"
				}
			],
			"gradients": [
				{
					"slug": "vertical-base-to-accent",
					"gradient": "linear-gradient(to bottom, var(--wp--custom--color--accent) 50%, var(--wp--preset--color--background) 50%)",
					"name": "Vertical base to accent"
				}
			],
			"palette": [
				{
					"slug": "primary",
					"color": "#abacca",
					"name": "Accent 1"
				},
				{
					"slug": "secondary",
					"color": "#a8bed2",
					"name": "Accent 2"
				},
                                {
                                        "slug": "tertiary",
                                        "color": "#a4beb2",
                                        "name": "Accent 3"
                                },
                                {
                                        "slug": "quarternary",
                                        "color": "#eaca96",
                                        "name": "Accent 4"
                                },
                                {
                                        "slug": "quinary",
                                        "color": "#e9aa95",
                                        "name": "Accent 5"
                                },
                                {
                                        "slug": "senary",
                                        "color": "#eeb8b0",
                                        "name": "Accent 6"
                                },
                                {
                                        "slug": "foreground",
                                        "color": "#000000",
                                        "name": "Contrast"
                                },
				{
					"slug": "foreground-alt",
					"color": "#555555",
					"name": "Contrast Alt"
				},
                                {
                                        "slug": "background",
                                        "color": "#ffffff",
                                        "name": "Base"
                                },
                                {
                                        "slug": "background-alt",
                                        "color": "#e7e7e9",
                                        "name": "Base Alt"
                                },
                                {
                                        "slug": "accent",
                                        "color": "var(--wp--custom--color--accent)",
                                        "name": "Post Accent"
                                }
			]
		},
		"custom": {
                        "color": {
                                "accent": "var(--wp--preset--color--primary)",
                                "code": "#ce365c"
                        },
			"img-radius": "10px",
			"typography": {
				"font-size": {
					"huge": "clamp(2.25rem, 4vw, 2.75rem)",
					"colossal": "clamp(4rem, 13vw, 10rem)"
				},
				"line-height": {
					"tiny": 1.15,
					"small": 1.2,
					"medium": 1.4,
					"normal": 1.725
				}
			}
		},
		"layout": {
			"contentSize": "40rem",
			"wideSize": "52rem"
		},
		"spacing": {
			"spacingScale": {
				"steps": 7
			},
			"spacingSizes": [
				{
					"size": "min(20px, 1vw)",
					"slug": "10",
					"name": "1"
				},
				{
					"size": "min(30px, 2vw)",
					"slug": "20",
					"name": "2"
				},
				{
					"size": "min(40px, 4vw)",
					"slug": "30",
					"name": "3 (Outer)"
				},
				{
					"size": "min(50px, 6vw)",
					"slug": "40",
					"name": "4"
				},
				{
					"size": "min(60px, 10vw)",
					"slug": "50",
					"name": "5"
				},
				{
					"size": "min(70px, 12vw)",
					"slug": "60",
					"name": "6"
				},
				{
					"size": "min(80px, 14vw)",
					"slug": "70",
					"name": "7"
				}
			]
		},
		"typography": {
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Inter",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "200 900",
							"src": [
								"file:./assets/fonts/Inter-VariableFont_slnt,wght.ttf"
							]
						}
					],
					"fontFamily": "\"Inter\", sans-serif",
					"name": "Inter",
					"slug": "body"
				}
			],
			"fontSizes": [
				{
					"size": "1.05rem",
					"slug": "small"
				},
				{
					"size": "clamp(1.05rem, 2vw, 1.275rem)",
					"slug": "medium"
				},
				{
					"size": "clamp(1.45rem, 2vw, 2rem)",
					"slug": "large"
				},
				{
					"size": "clamp(1.75rem, 3vw, 2.25rem)",
					"slug": "x-large",
					"name": "1X Large"
				},
                                {
                                        "size": "clamp(2.25rem, 8.5vw, 5rem)",
                                        "slug": "xx-large",
                                        "name": "2X Large"
                                }
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
                        "core/post-title": {
                                "typography": {
                                        "lineHeight": "var(--wp--custom--typography--line-height--tiny)",
                                        "fontSize": "var(--wp--preset--font-size--xx-large)"
                                }
                        },
                        "core/navigation": {
                                "typography": {
                                        "fontSize": "var(--wp--preset--font-size--small)"
                                }
                        },
                        "core/search": {
                                "border": {
                                        "radius": "12px"
                                },
                                "typography": {
                                        "fontSize": "1rem"
                                }
                        },
                        "core/code": {
                                "border": {
                                        "width": "0",
                                        "radius": "10px"
                                },
                                "color": {
                                        "background": "var(--wp--preset--color--background-alt)",
                                        "text": "var(--wp--preset--color--foreground)"
                                },
                                "spacing": {
                                        "padding": {
                                                "top": "var(--wp--preset--spacing--30)",
                                                "bottom": "var(--wp--preset--spacing--30)",
                                                "right": "var(--wp--preset--spacing--30)",
                                                "left": "var(--wp--preset--spacing--30)"
                                        }
                                },
                                "typography": {
                                        "lineHeight": "2",
                                        "fontSize": "clamp(0.85rem, 1vw, 1rem)",
                                        "fontFamily": "monospace"
                                }
                        },
			"core/post-comments": {
				"spacing": {
					"padding": {
						"top": "var(--wp--preset--spacing--20)"
					}
				}
			},
			"core/pullquote": {
				"border": {
					"width": "1px 0"
				}
			},
			"core/query-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"core/quote": {
				"border": {
					"width": "1px"
				}
			},
			"core/site-title": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--foreground)"
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--body)",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontWeight": "600"
				}
			},
			"tabor/share-on-twitter": {
				"border": {
					"radius": "10px",
					"color": "var(--wp--preset--color--background-alt)",
					"width": "2px"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "var(--wp--custom--typography--line-height--medium)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"button" : {
				"border": {
					"radius": "8px"
				},
				"color": {
					"background": "var(--wp--preset--color--foreground)",
					"text": "var(--wp--preset--color--background)"
				},
				"spacing": {
					"padding": {
						"top": "min(1rem, 1.75vw)",
						"right": "min(1.75rem, 4vw)",
						"bottom": "min(1rem, 1.75vw)",
						"left": "min(1.75rem, 4vw)"
					}
				},
				"typography": {
					"fontSize": "1.15rem"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--foreground-alt)",
						"text": "var(--wp--preset--color--background)"
					}
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--foreground-alt)"
				},
				"typography": {
					"fontSize": "14px"
				}
			},
			"h1": {
                                "color": {
                                        "text": "var(--wp--preset--color--foreground)"
                                },
				"typography": {
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"letterSpacing": "-0.025em"
				}
			},
			"h2": {
                                "color": {
                                       "text": "var(--wp--preset--color--foreground)"
                                },
				"typography": {
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"letterSpacing": "-0.025em"
				}
			},
			"h3": {
                                "color": {
                                        "text": "var(--wp--preset--color--foreground)"
                                },
				"typography": {
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--small)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"letterSpacing": "-0.055em"
				}
			},
			"h4": {
                                "color": {
                                        "text": "var(--wp--preset--color--foreground)"
                                },
				"typography": {
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--small)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"letterSpacing": "-0.055em"
				}
			},
			"h5": {
                                "color": {
                                        "text": "var(--wp--preset--color--foreground-alt)"
                                },
				"typography": {
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
                                "color": {
                                        "text": "var(--wp--preset--color--foreground)"
                                },
				"typography": {
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"fontSize": "var(--wp--preset--font-size--small)",
					"textTransform": "uppercase",
					"letterSpacing": "0.05rem"
				}
			},
			"link": {
                                "color": {
                                        "text": "var(--wp--preset--color--foreground)"
                                },
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--foreground)"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--30)",
			"padding": {
				"right": "var(--wp--preset--spacing--30)",
				"left": "var(--wp--preset--spacing--30)"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--body)",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)",
			"fontSize": "var(--wp--preset--font-size--medium)",
			"fontWeight": "400"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "header-accent",
			"title": "Header (Accent)",
			"area": "header"
		},
		{
			"name": "header-w-button",
			"title": "Header (with Button)",
			"area": "header"
		},
		{
			"name": "header-accent-w-button",
			"title": "Header (Accent with Button)",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		},
		{
			"name": "footer-w-posts",
			"title": "Footer (Posts)",
			"area": "footer"
		},
		{
			"name": "loop",
			"title": "Loop",
			"area": "uncategorized"
		},
		{
			"name": "loop-alt",
			"title": "Loop, Alternate",
			"area": "uncategorized"
		}
	]
}
